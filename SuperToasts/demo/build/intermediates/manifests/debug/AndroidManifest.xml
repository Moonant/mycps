<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.supertoastsdemo"
    android:versionCode="16"
    android:versionName="1.3.4" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.supertoastsdemo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.supertoastsdemo.ActivityTwo" />
        <activity android:name="com.supertoastsdemo.Playground" />
        <activity android:name="com.supertoastsdemo.examples.ExampleUndoBar" />
        <activity android:name="com.supertoastsdemo.examples.ExampleStyle" />
        <activity android:name="com.supertoastsdemo.examples.ExampleSuperActivityToast" />
    </application>

</manifest>